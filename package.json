{
  "name": "generator-cheetah",
  "version": "1.0.9",
  "description": "A cleaner alternative to the popular create-react-app, providing a ready to go webpack reactjs app with webpack optimisation for css, sass, scss and javascript files.",
  "main": "generators/app/index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open --hot",
    "release": "webpack --config webpack.release.js",
    "build": "webpack --config webpack.prod.js"
  },
  "keywords": [
    "yeoman-generator",
    "webpack",
    "styles",
    "bundler",
    "css",
    "scss",
    "sass"
  ],
  "files": [
    "generators/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:MarcAdlington1/webpack-style-bundler.git"
  },
  "author": "Marc Adlington",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "yeoman-generator": "^4.0.1",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  }
}
